    const baseValueOld = 197;
    const baseValuePromo = 127;

    const origin = getOrigin();

    updateOfferBlack(1)
    updateOfferWhite(1)

    //white
    $('.white-plus').click(function (e) {
      // Stop acting like a button
      e.preventDefault();
      // Get the field name
      const quantity = parseInt($('#quantity-white').val());
      // If is not undefined
      if (quantity >= whiteUrlArray.length) {
        return;
      }
      const newValue = quantity + 1;
      $('#quantity-white').val(newValue);
      updateOfferWhite(newValue);
      // Increment
    });

    $('.white-minus').click(function (e) {
      // Stop acting like a button
      e.preventDefault();
      // Get the field name
      var quantity = parseInt($('#quantity-white').val());
      // If is not undefined
      // Increment
      if (quantity > 1) {
        const newValue = quantity - 1;
        $('#quantity-white').val(newValue);
        updateOfferWhite(newValue);
      }
    });

    //black
    $('.black-plus').click(function (e) {
      // Stop acting like a button
      e.preventDefault();
      // Get the field name
      var quantity = parseInt($('#quantity-black').val());
      // If is not undefined
      if (quantity >= blackUrlArray.length) {
        return;
      }
      const newValue = quantity + 1;
      $('#quantity-black').val(newValue);
      updateOfferBlack(newValue);
      // Increment
    });

    $('.black-minus').click(function (e) {
      // Stop acting like a button
      e.preventDefault();
      // Get the field name
      var quantity = parseInt($('#quantity-black').val());
      // If is not undefined
      // Increment
      if (quantity > 1) {
        const newValue = quantity - 1;
        $('#quantity-black').val(newValue);
        updateOfferBlack(newValue);
      }
    });

    function updateOfferBlack(qty) {
      let referenceArray = blackUrlArray;

      if (instagramTraffic(origin)) {
        referenceArray = blackUrlInstagram;




      }

      if (facebookTraffic(origin)) {
        referenceArray = blackUrlFacebook;
      }

      const href = referenceArray[qty - 1]
      updateBlackButton(href);
      const priceOld = qty * baseValueOld;
      const pricePromo = qty * baseValuePromo;
      $('#price-black-old').text(priceOld);
      $('#price-black-promo').text(pricePromo);

    }

    function updateOfferWhite(qty) {
      let referenceArray = whiteUrlArray;


      if (instagramTraffic(origin)) {
        referenceArray = whiteUrlInstagram;



      }

      if (facebookTraffic(origin)) {
        referenceArray = whiteUrlFacebook;
      }

      const href = referenceArray[qty - 1]
      updateWhiteButton(href);
      const priceOld = qty * baseValueOld;
      const pricePromo = qty * baseValuePromo;
      $('#price-black-old').text(priceOld);
      $('#price-black-promo').text(pricePromo);

    }

    function updateBlackButton(href) {
      $('#buy-black').attr('href', href);
    }

    function updateWhiteButton(href) {
      $('#buy-white').attr('href', href);
    }
  })



</script>

<script>
function selectColor(evt, colorSelector) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName("tabcontent");
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("tablinks");
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].className = tablinks[i].className.replace(" active", "");
  }
  document.getElementById(colorSelector).style.display = "block";
  evt.currentTarget.className += " active";
}
</script>

    <script>
    $(document).scroll(function() {
  var y = $(this).scrollTop();
  if (y > 1000) {
    $('.bottomMenu').fadeIn();
  } else {
    $('.bottomMenu').fadeOut();
  }
});
    </script>

    <script>
    
      function hasTouch() {
  return 'ontouchstart' in document.documentElement
         || navigator.maxTouchPoints > 0
         || navigator.msMaxTouchPoints > 0;
}

if (hasTouch()) { // remove all the :hover stylesheets
  try { // prevent exception on browsers not supporting DOM styleSheets properly
    for (var si in document.styleSheets) {
      var styleSheet = document.styleSheets[si];
      if (!styleSheet.rules) continue;

      for (var ri = styleSheet.rules.length - 1; ri >= 0; ri--) {
        if (!styleSheet.rules[ri].selectorText) continue;

        if (styleSheet.rules[ri].selectorText.match(':hover')) {
          styleSheet.deleteRule(ri);
        }
      }
    }
  } catch (ex) {}
}
  
    </script>


